                  <div class="compare__left">
                    <div class="compare__icon">{{ icon.make('compare-left') }}</div>
                    <div class="compare__numb">92 250</div>
                    <div class="compare__text">ETH</div>
                  </div>
                  <div class="compare__right">
                    <div class="compare__numb">1 180 000</div>
                    <div class="compare__text">+ 236 000 (BONUS)</div>
                    <div class="compare__text">LNT</div>
                    <div class="compare__icon">{{ icon.make('compare-right') }}</div>
                  </div>
                </div>
                <!-- end compare -->
              </div>
