{% extends "layout.njk" %}

{% block content %}

  <!-- begin contact -->
  <div class="contact">
    <div class="contact__container container-fluid">
      {{ breadcrumbs.make({
        list: ['ICO Project’s', 'Portfoli', 'LancerNetwo', 'Contract_#1']
      }) }}
      <div class="box box--has--head">
        <div class="box__head box__head--md">
          <div class="box__head-inner">
            <div class="box__head-left">
              <div class="date">
                <span class="date__label">Date:</span> 01.08.2018
              </div>
            </div>
            <div class="box__head-center text-center">
              <h2 class="text-blue">Lancer Network</h2>
              <h2 class="text-uppercase">INVESTMENT CONTRACT</h2>
            </div>
            <div class="box__head-right">
              <div class="mb-1"><a href="#" class="source-link">{{ icon.make('link') }} Source FIle of Smart Contract</a></div>
              <div class="mb-0"><a href="#" class="source-link">{{ icon.make('link') }} PDF File of Smart Contract</a></div>
            </div>
          </div>
        </div>
        <div class="box__wrap box__wrap--2md">
          <!-- begin sale -->
          <div class="sale">
            <div class="sale__head">
              <div class="sale__left">
                <!-- begin user -->
                <div class="user user--check-right">
                  <div class="user__wrap">
                    <div class="user__avatar" style="background-image: url(./assets/imgs/user-avatar.png);">
                      <div class="user__check-line">{{ icon.make('check-valid') }}</div>
                    </div>
                    <div class="user__name">David G.</div>
                  </div>
                </div>
                <!-- end user -->
              </div>
              <div class="sale__center">
                <!-- begin sale-card -->
                <div class="sale-card">
                  <div class="sale-card__head">
                    <div class="sale-card__head-title">PRIVATE SALE</div>
                    <div class="sale-card__date">01.01 - 02.02.2019</div>
                  </div>
                  <div class="sale-card__wrap">
                    <div class="sale-card__description">Agreement of Token Purchase:</div>
                    <div class="sale-card__summ">1 180 000 LNT</div>
                    <div class="sale-card__bonus">+ 20% BONUS</div>
                    <div class="sale-card__conversion">1 LNT = 0.00025 ETH = 25 USD</div>
                  </div>
                </div>
                <!-- end sale-card -->
              </div>
              <div class="sale__right">
                <!-- begin user -->
                <div class="user user--check-left">
                  <div class="user__wrap">
                    <div class="user__avatar" style="background-image: url(./assets/imgs/user-avatar-2.png);">
                      <div class="user__check-line">{{ icon.make('check-valid') }}</div>
                    </div>
                    <div class="user__name">LancerNetwork <span class="user__status is-active"></span></div>
                  </div>
                </div>
                <!-- end user -->
              </div>
            </div>
            <div class="sale__body">
              <!-- begin box -->
              <div class="sale__box box box--p--lg box--border">
                <div class="box__control">
                  <a href="#" class="copy-link"><span class="copy-link__text"></span>0x123f681646d4a755815f9cb19e1acc8565a0c2ac</a>
                </div>
                <div class="box__cols">
                  <div class="box__row">
                    <div class="box__col">David Pierre Guetta</div>
                    <div class="box__col"><a href="mailto:info@lancer.network" class="icon-link">{{ icon.make('email') }}info@lancer.network</a></div>
                  </div>
                </div>
              </div>
              <!-- end box -->
              <div class="sale__compare">
